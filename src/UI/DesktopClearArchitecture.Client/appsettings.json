{
    "Serilog": {
        "Using":  
        [ 
            "Serilog.Sinks.File",
            "Serilog.Sinks.Debug"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": 
        [
            { 
                "Name": "File",
                "Args": 
                { 
                    "path": "log\\log.txt", 
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Debug"
            }
        ]
    }
}